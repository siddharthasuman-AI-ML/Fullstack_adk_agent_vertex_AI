# =============================================================================
# ADK Agent Engine Deployment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
GOOGLE_GENAI_USE_VERTEXAI=True

# Required: Your Google Cloud Project ID
GOOGLE_CLOUD_PROJECT=your-project-id

# Required: Google Cloud region (us-central1 is recommended)
GOOGLE_CLOUD_LOCATION=us-central1

# Required: Name for your agent (used both internally and for deployment)
AGENT_NAME=goal-planning-agent

# Optional: Google Cloud Storage bucket for staging (will be created if not exists)
# GOOGLE_CLOUD_STAGING_BUCKET=your-staging-bucket

# Optional: AI model to use (default: gemini-2.5-flash)
# MODEL=gemini-2.5-flash

# Optional: Requirements file path (default: .requirements.txt)
# REQUIREMENTS_FILE=.requirements.txt

# Optional: Extra packages to include in deployment (default: ./app)
# EXTRA_PACKAGES=./app

# PRODUCITON
AGENT_ENGINE_ENDPOINT=
GOOGLE_SERVICE_ACCOUNT_KEY_BASE64=
